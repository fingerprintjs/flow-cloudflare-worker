{
  "name": "flow-cloudflare-worker",
  "version": "0.0.1",
  "description": "Cloudflare Worker for Flow",
  "author": "FingerprintJS, Inc (https://fingerprint.com)",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/flow-cloudflare-worker.git"
  },
  "scripts": {
    "preview:react-spa": "cd test-apps/react-spa && pnpm preview",
    "dev": "vite serve --config vite.worker.ts --mode dev",
    "prepare": "husky && ./scripts/setup-wrangler.sh && pnpm build:instrumentor",
    "build": "pnpm build:worker && pnpm build:instrumentor",
    "build:worker": "vite build --config vite.worker.ts",
    "build:instrumentor": "vite build --config vite.instrumentor.ts",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:attach": "vitest --inspect=9229 --no-file-parallelism",
    "wrangler:deploy": "pnpm build:worker && wrangler deploy",
    "wrangler:types": "wrangler types --strict-vars=false"
  },
  "lint-staged": {
    "*.{js,ts}": "pnpm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.3",
    "@cloudflare/vitest-pool-workers": "^0.9.5",
    "@commitlint/cli": "^19.8.0",
    "@fingerprintjs/commit-lint-dx-team": "^0.1.0",
    "@fingerprintjs/conventional-changelog-dx-team": "^0.1.0",
    "@fingerprintjs/eslint-config-dx-team": "^0.1.0",
    "@fingerprintjs/fingerprintjs-pro": "^3.12.2",
    "@fingerprintjs/prettier-config-dx-team": "^0.2.0",
    "@fingerprintjs/tsconfig-dx-team": "^0.0.2",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "happy-dom": "^19.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-checker": "^0.10.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.40.2"
  }
}
