{
  "name": "flow-cloudflare-worker",
  "version": "0.0.1",
  "description": "Cloudflare Worker for Flow",
  "author": "FingerprintJS, Inc (https://fingerprint.com)",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerprintjs/library-template-typescript.git"
  },
  "scripts": {
    "serve": "vite",
    "prepare": "husky && pnpm build:injector",
    "build": "vite build",
    "build:injector": "vite build --config vite.injector.ts",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:typecheck": "tsc -p tsconfig.typecheck.json",
    "wrangler:deploy": "pnpm build && wrangler deploy"
  },
  "main": "dist/flow-cloudflare-worker.cjs.js",
  "module": "dist/flow-cloudflare-worker.esm.js",
  "types": "dist/flow-cloudflare-worker.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{js,ts}": "pnpm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.3",
    "@cloudflare/workers-types": "^4.20250923.0",
    "@commitlint/cli": "^19.8.0",
    "@fingerprintjs/commit-lint-dx-team": "^0.1.0",
    "@fingerprintjs/conventional-changelog-dx-team": "^0.1.0",
    "@fingerprintjs/eslint-config-dx-team": "^0.1.0",
    "@fingerprintjs/prettier-config-dx-team": "^0.2.0",
    "@fingerprintjs/tsconfig-dx-team": "^0.0.2",
    "@types/node": "^24.5.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
